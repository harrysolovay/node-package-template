{
    "arrowParens": "always",
    "bracketSpacing": false,
    "embeddedLanguageFormatting": "auto",
    "importOrder": ["^@/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "printWidth": 999,
    "proseWrap": "never",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false
}
