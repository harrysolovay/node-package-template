{
  "arrowParens": "always",
  "bracketSpacing": false,
  "embeddedLanguageFormatting": "auto",
  "importOrder": ["^@/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "printWidth": 999,
  "proseWrap": "never",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
